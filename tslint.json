{
	"rules": {
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"no-duplicate-key": true,
		"no-unused-variable": true,
		"no-eval": true,
		"no-internal-module": true,
		"no-trailing-whitespace": [true, "ignore-jsdoc", "ignore-comments", "ignore-template-strings"],
		"no-var-keyword": true,
		"curly": true,
		"class-name": true,
		"semicolon": [true, "never"],
		"triple-equals": [true, "allow-null-check"],
		"indent": [true, "spaces", 2],
		"one-line": [
				true,
				"check-catch",
				"check-else",
				"check-open-brace",
				"check-whitespace"
		],
		"quotemark": [false, "single", "jsx-double"],
		"interface-name": [true, "never-prefix"],
		"enum": [true, "never-prefix"],
		"member-access": [true, "no-public"],
		"member-ordering": [
				true,
				"public-before-private",
				"static-before-instance",
				"variables-before-functions"
		],
		"only-arrow-functions": [true, "allow-declarations", "allow-named-functions"],
		"prefer-for-of": false,
		"promise-function-async": true,
		"unified-signatures": true,
		"await-promise": true,
		"no-arg": true,
		"no-bitwise": true,
		"no-conditional-assignment": true,
		"no-construct": true,
		"no-duplicate-super": true,
		"no-floating-promises": true,
		"no-for-in-array": true,
		"no-invalid-this": false,
		"no-misused-new": true,
		"no-string-throw": true,
		"no-switch-case-fall-through": true,
		"no-unsafe-finally": true,
		"no-use-before-declare": true,
		"prefer-conditional-expression": true,
		"prefer-object-spread": true,
		"radix": true,
		"restrict-plus-operands": true,
		"typeof-compare": true,
		"use-default-type-parameter": true,
		"use-isnan": true,
		"cyclomatic-complexity": [false, 20],
		"deprecation": true,
		"eofline": false,
		"max-classes-per-file": [true, 1],
		"no-default-export": false,
		"prefer-const": true,
		"trailing-comma": [false, {"multiline": "always", "singleline": "never"}],
		"arrow-return-shorthand": true,
		"binary-expression-operand-order": true,
		"encoding": true,
		"interface-over-type-literal": true,
		"jsdoc-format": true,
		"new-parens": true,
		"no-unnecessary-callback-wrapper": true,
		"no-unnecessary-initializer": true,
		"no-unnecessary-qualifier": true,
		"object-literal-shorthand": true,
		"one-variable-per-declaration": [true, "ignore-for-loop"],
		"variable-name": [true, "ban-keywords", "check-format", "allow-snake-case"],
		"whitespace": [
				true,
				"check-branch",
				"check-decl",
				"check-module",
				"check-operator",
				"check-separator",
				"check-type",
				"check-typecast"
		]
	}
}